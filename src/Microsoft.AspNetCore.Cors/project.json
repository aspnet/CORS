{
  "version": "1.0.2",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnet/cors"
    },
    "tags": [
      "aspnetcore",
      "cors"
    ]
  },
  "description": "CORS middleware and policy for ASP.NET Core to enable cross-origin resource sharing.\r\nCommonly used types:\r\nMicrosoft.AspNetCore.Cors.DisableCorsAttribute\r\nMicrosoft.AspNetCore.Cors.EnableCorsAttribute",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Http.Extensions": "1.0.2",
    "Microsoft.Extensions.Configuration.Abstractions": "1.0.2",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.1",
    "Microsoft.Extensions.Options": "1.0.2"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {}
  }
}